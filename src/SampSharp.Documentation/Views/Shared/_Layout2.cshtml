<main>
    <div class="content-heading">
        <h1>@ViewData["Title"]</h1>

        @RenderSection("Introduction", false)
    </div>

    @if (IsSectionDefined("InThisArticleInline"))
    {
        <div class="article-nav-inline article-nav">
            <div class="header">
                <span>In this article</span>
            </div>
            <ul class="content">
                @RenderSection("InThisArticleInline")
            </ul>
        </div>
    }

    <div class="content-body">
        @RenderBody()
    </div>
</main>

@if (IsSectionDefined("InThisArticle"))
{
    <div class="article-nav-container article-nav">
        <div class="can-float">
            <div class="float-header header">
                <span>In this article</span>
            </div>
            <ul class="float-content content">
                @RenderSection("InThisArticle")
            </ul>
        </div>
    </div>
}
